{
  "name": "Linkclump",
  "version": "1.0.3",
  "description": "Opens all selected links into new tabs (works on windows/mac/linux).",
  "background_page": "background.html",
  "icons": {"16": "favicon.ico",
           "48": "48x48.png",
          "128": "128x128.png" },
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["content_script.js"],
    "run_at": "document_end",
    "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Linkclump",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
