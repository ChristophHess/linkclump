{
  "name": "Linkclump - Open all selected links",
  "version": "1.0.6",
  "description": "Use your mouse to select and open groups of links into new tabs [works on windows/mac/linux]",
  "background_page": "background.html",
  "options_page": "options.html",
  "icons": {"16": "16x16.png",
           "48": "48x48.png",
          "128": "128x128.png" },
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content_script.js", "jquery-1.4.4.js"],
    "run_at": "document_end",
    "all_frames": true
    }
  ],
  "permissions": [
    "tabs"
  ]
}
